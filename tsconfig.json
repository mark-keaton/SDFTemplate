{
  "compilerOptions": {
    "allowJs": true,
    "moduleResolution": "node",
    "module": "amd",
    "target": "es5",
    "sourceMap": false,
    "noImplicitUseStrict": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ],
    "rootDir": "./src/",
    "outDir": "./FileCabinet/SuiteScripts",
    "paths": {
      "N": [
        "node_modules/@hitc/netsuite-types/N"
      ],
      "N/*": [
        "node_modules/@hitc/netsuite-types/N/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/"
  ]
}
